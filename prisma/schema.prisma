generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Work {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  cover     String
  like      Int
  comment   String
  link      String
  createdAt DateTime @default(now())
}
